{
  "name": "youtube-timestamp",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest"
  },
  "workspaces": [
    "apps/web",
    "apps/api"
  ],
  "scripts": {
    "dev": "bun run dev:web & bun run dev:api",
    "dev:web": "cd apps/web && bun dev",
    "dev:api": "cd apps/api && bun --env-file=.env --hot run index.ts"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "googleapis": "^171.0.0",
    "openai": "^6.17.0"
  }
}
